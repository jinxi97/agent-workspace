CLUSTER_NAME="agent-workspace-cluster"
GKE_LOCATION="us-central1"
GKE_VERSION="1.35.0-gke.2232003"
AGENT_SANDBOX_VERSION="v0.1.1"
SANDBOX_ROUTER_IMAGE="us-central1-docker.pkg.dev/k8s-staging-images/agent-sandbox/sandbox-router:v20260224-v0.1.1.post3-8-gf849dc2"
NODE_POOL_NAME="agent-workspace-node-pool"
MACHINE_TYPE="n2-standard-2"
SNAPSHOTS_BUCKET_NAME_PREFIX="agent-sandbox-snapshots-"
SNAPSHOT_FOLDER="workspace-snapshots"
SNAPSHOT_NAMESPACE="pod-snapshots-ns"
SNAPSHOT_KSA_NAME="pod-snapshot-sa"
SANDBOX_TEMPLATE_REVISION=v7
WORKLOADS_NAMESPACE="agent-sandbox-application"

# FastAPI deployment
FASTAPI_APP_NAME="agent-workspace-api"
FASTAPI_CONTAINER_PORT=8080
FASTAPI_SERVICE_PORT=80

# Cloud Build trigger (GitHub App, Gen2 only)
CLOUDBUILD_FILE="cloudbuild.yaml"
CLOUDBUILD_BRANCH_NAME="main"
CLOUDBUILD_LOCATION="us-central1"
CLOUDBUILD_REPOSITORY="projects/<project-id>/locations/<location>/connections/<connection-name>/repositories/<repository-name>"
